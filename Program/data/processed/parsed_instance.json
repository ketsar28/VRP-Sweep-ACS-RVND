  "formulas": {
    "polar_angle": "theta_i = atan2(y_i - y_0, x_i - x_0)",
    "euclidean_distance": "d_ij = sqrt((x_i - x_j)^2 + (y_i - y_j)^2)",
    "travel_time": "t_ij = d_ij (vehicle speed = 1 km per minute)",
    "arrival_time": "A_j = max(a_j, A_i + s_i + t_ij)",
    "time_window_violation": "V_j = max(0, A_j - b_j)",
    "visibility": "eta_ij = 1 / d_ij for d_ij > 0",
    "transition_probability": "If q <= q0, choose argmax of tau_ij^alpha * eta_ij^beta; else p_ij = (tau_ij^alpha * eta_ij^beta) / sum_{k in N_i}(tau_ik^alpha * eta_ik^beta)",
    "local_pheromone_update": "tau_ij = (1 - rho) * tau_ij + rho * tau0",
    "global_pheromone_update": "For edges in best route: tau_ij = (1 - rho) * tau_ij + rho * (1 / L_best); others: tau_ij = (1 - rho) * tau_ij",
    "objective_function": "F = alpha * D + beta * T + gamma * V",
    "route_cost": "cost = fixed_cost + variable_cost_per_km * distance",
    "initial_pheromone": "tau0 = 1 / (n_customers * nn_length)"
  },
  "assumptions": {
    "vehicle_speed_km_per_min": 1,
    "waiting_allowed": true,
    "time_window_policy": "soft",
    "tw_violation_expression": "V_j = max(0, A_j - b_j)",
    "cluster_vehicle_rule": "One cluster equals one vehicle; choose vehicle type based on cluster demand within available units"
  }
