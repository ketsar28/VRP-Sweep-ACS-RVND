  "formulas": {
    "polar_angle": "theta_i = atan2(y_i - y_0, x_i - x_0)",
    "euclidean_distance": "d_ij = sqrt((x_i - x_j)^2 + (y_i - y_j)^2)",
    "travel_time": "t_ij = d_ij (vehicle speed = 1 km per minute)",
    "arrival_time": "A_j = max(a_j, A_i + s_i + t_ij)",
